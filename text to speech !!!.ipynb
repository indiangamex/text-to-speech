     "text": [
      "enter your desired text :you'll find it their !!\n"
     ]
    }
   ],
   "source": [
    "text = input(\"enter your desired text :\")\n",
    "import pyttsx3\n",
    "engine = pyttsx3.init()\n",
    "voices = engine.getProperty('voices')\n",
    "engine.setProperty('rate', 100)    \n",
    "engine.setProperty('voice',voices[1].id)\n",
    "engine.say(text)\n",
    "engine.runAndWait()"
   ]
